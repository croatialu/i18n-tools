{
  "name": "@croatialu/i18n-tools",
  "type": "module",
  "version": "0.0.0",
  "publishConfig": {
    "access": "public",
    "tag": "latest",
    "registry": "https://registry.npmjs.org/"
  },
  "packageManager": "pnpm@9.12.2",
  "description": "_description_",
  "author": "Croatia Lu <wuli.croatia@foxmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/croatialu",
  "homepage": "https://github.com/croatialu/i18n-tools#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/croatialu/i18n-tools.git"
  },
  "bugs": "https://github.com/croatialu/i18n-tools/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "i18n": "bin/index.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno watch src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "consola": "^3.2.3",
    "lodash-es": "^4.17.21",
    "ora": "^8.1.0",
    "tinyexec": "^0.3.1",
    "unconfig": "^0.6.0",
    "which": "^5.0.0",
    "yaml": "^2.6.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@antfu/ni": "^0.23.0",
    "@antfu/utils": "^0.7.10",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.7.6",
    "@types/which": "^3.0.4",
    "bumpp": "^9.7.1",
    "eslint": "^9.12.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.12.2",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "vite": "^5.4.9",
    "vitest": "^2.1.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
